{
  "name": "afs",
  "productName": "afs",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "electron-forge start",
    "package-windows": "electron-forge package --arch x64 --platform win32 && cp -R base out/afs-win32-x64/storage",
    "package-linux": "electron-forge package --arch x64 --platform linux && cp -R base out/afs-linux-x64/storage",
    "package-mac": "electron-forge package --arch x64 --platform darwin",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js src tests",
    "backup": "cp -R storage ../backups/$(date +'%Y-%m-%d')"
  },
  "keywords": [],
  "author": {
    "name": "Matheus Borges Teixeira",
    "email": "matheus.bt@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "afs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "7.11.1",
    "@electron-forge/maker-deb": "7.11.1",
    "@electron-forge/maker-rpm": "7.11.1",
    "@electron-forge/maker-squirrel": "7.11.1",
    "@electron-forge/maker-zip": "7.11.1",
    "@electron-forge/plugin-webpack": "7.11.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "30.0.0",
    "@types/react-dom": "19.2.3",
    "@types/react-redux": "7.1.33",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-mock-store": "1.5.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "css-loader": "7.1.3",
    "electron": "40.1.0",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "node-loader": "2.1.0",
    "redux-mock-store": "1.5.5",
    "style-loader": "4.0.0",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@redux-devtools/extension": "3.3.0",
    "canvas": "3.2.1",
    "chart.js": "4.5.1",
    "electron-squirrel-startup": "1.0.1",
    "evergreen-ui": "7.1.9",
    "react": "19.2.4",
    "react-chartjs-2": "5.3.1",
    "react-dom": "19.2.4",
    "react-redux": "9.2.0",
    "react-router-dom": "6.30.3",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0",
    "uuid": "13.0.0"
  }
}
