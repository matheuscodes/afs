{
  "name": "afs",
  "productName": "afs",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package-windows": "electron-forge package --arch x64 --platform win32 && cp -R base out/afs-win32-x64/storage",
    "package-linux": "electron-forge package --arch x64 --platform linux && cp -R base out/afs-linux-x64/storage",
    "package-mac": "electron-forge package --arch x64 --platform darwin",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js .",
    "backup": "cp -R storage ../backups/$(date +'%Y-%m-%d')"
  },
  "keywords": [],
  "author": {
    "name": "Matheus Borges Teixeira",
    "email": "matheus.bt@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "afs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.4",
    "@electron-forge/maker-deb": "6.0.4",
    "@electron-forge/maker-rpm": "6.0.4",
    "@electron-forge/maker-squirrel": "6.0.4",
    "@electron-forge/maker-zip": "6.0.4",
    "@electron-forge/plugin-webpack": "6.0.4",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/electron": "1.6.10",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "css-loader": "4.3.0",
    "electron": "22.1.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "fork-ts-checker-webpack-plugin": "5.2.1",
    "node-loader": "1.0.2",
    "style-loader": "1.3.0",
    "ts-loader": "8.0.17",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@types/fuzzaldrin-plus": "0.6.1",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/uuid": "8.3.0",
    "chart.js": "4.2.0",
    "electron-squirrel-startup": "1.0.0",
    "evergreen-ui": "7.0.1",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.8.0",
    "redux": "4.2.1",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.4.2"
  }
}
