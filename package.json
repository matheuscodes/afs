{
  "name": "afs",
  "productName": "afs",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package-windows": "electron-forge package --arch x64 --platform win32 && cp -R base out/afs-win32-x64/storage",
    "package-linux": "electron-forge package --arch x64 --platform linux && cp -R base out/afs-linux-x64/storage",
    "package-mac": "electron-forge package --arch x64 --platform darwin",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js .",
    "backup": "cp -R storage ../backups/$(date +'%Y-%m-%d')"
  },
  "keywords": [],
  "author": {
    "name": "Matheus Borges Teixeira",
    "email": "matheus.bt@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "afs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "7.4.0",
    "@electron-forge/maker-deb": "7.4.0",
    "@electron-forge/maker-rpm": "7.4.0",
    "@electron-forge/maker-squirrel": "7.4.0",
    "@electron-forge/maker-zip": "7.4.0",
    "@electron-forge/plugin-webpack": "7.4.0",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@types/electron": "1.6.10",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "css-loader": "7.1.1",
    "electron": "30.0.1",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "node-loader": "2.0.0",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@redux-devtools/extension": "3.3.0",
    "@types/fuzzaldrin-plus": "0.6.5",
    "@types/react-redux": "7.1.33",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "9.0.8",
    "chart.js": "4.4.2",
    "electron-squirrel-startup": "1.0.0",
    "evergreen-ui": "7.1.9",
    "react": "18.3.1",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.2",
    "react-router-dom": "6.23.0",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0"
  }
}
